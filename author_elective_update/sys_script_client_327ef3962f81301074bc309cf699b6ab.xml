    <sys_script_client action="DELETE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description>Only display What needed choices that match the Request type value.</description>
        <field>u_request_type</field>
        <global>false</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>NeedIt Request Type Options</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (newValue == '') {
      return;
   }
	g_form.clearOptions('u_what_need');

   //Type appropriate comment here, and begin script below
	var whatneeded = g_form.getValue('u_what_need');
	
	if (newValue == 'hr'){
			g_form.addOption('u_what_need', 'hr1', "Human Resources 1");
			g_form.addOption('u_what_need', 'hr2', "Human Resources 2");
			g_form.addOption('u_what_need', 'other', "Other");
	}
		
		
	if (newValue == 'facilities'){
			g_form.addOption('u_what_need', 'facilities1', "Facilities 1");
			g_form.addOption('u_what_need', 'facilities2', "Facilities 2");
			g_form.addOption('u_what_need', 'other', "Other");
	}
		
		
	if (newValue == 'legal'){
			g_form.addOption('u_what_need', 'legal1', "Legal 1");
			g_form.addOption('u_what_need', 'legal2', "Legal 2");
			g_form.addOption('u_what_need', 'other', "Other");
	}
	
	if(isLoading && !g_form.isNewRecord()){
				g_form.setValue('u_what_needed', whatneeded);
			}


				
	
}]]></script>
